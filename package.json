{
    "name": "wux-weapp-snippets",
    "displayName": "Wux Weapp Snippets",
    "description": "Wux Weapp Snippets for VS Code.",
    "icon": "wux-weapp-snippets.png",
    "version": "0.0.3",
    "homepage": "https://github.com/wux-weapp/wux-weapp-snippets/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/wux-weapp/wux-weapp-snippets/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wux-weapp/wux-weapp-snippets.git"
    },
    "publisher": "skyvow",
    "author": "skyvow",
    "license": "MIT",
    "engines": {
        "vscode": "^1.32.0"
    },
    "keywords": [
        "weapp",
        "wxml",
        "snippet"
    ],
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "wxml",
                "aliases": [
                    "wxml",
                    "wxml"
                ],
                "extensions": [
                    ".wxml"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "wxml",
                "scopeName": "text.html.wxml",
                "path": "./syntaxes/wxml.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "wxml",
                "path": "./snippets/wxml.json"
            },
            {
                "language": "html",
                "path": "./snippets/wxml.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/script.json"
            }
        ]
    }
}